      <con:properties>
        <con:property>
          <con:name>Username</con:name>
          <con:value>Loginn</con:value>
        </con:property>
        <con:property>
          <con:name>Password</con:name>
          <con:value>Loginn123</con:value>
        </con:property>
      </con:properties>
    </con:config>
  </con:testStep>
  <con:testStep type="request" name="Test Request - login" id="39aca9e4-ef87-44ce-ad06-4ce52f002b57">
